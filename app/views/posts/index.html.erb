
<h1>All posts in index view</h1>

<% if session[:current_user_id] %>
<%= link_to "Logout", logout_path %>
<%= link_to "create new post", new_post_path %>
<% else %>
<%= link_to "Login", login_path %>
<%= link_to "Signup", signup_path %>
<% end %>


<% @posts.each do |post| %>
<li><%= link_to post.title, post %></li>

<%end%>


